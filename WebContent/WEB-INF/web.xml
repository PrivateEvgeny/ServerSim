<!DOCTYPE web-app 
PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN" 
"http://java.sun.com/j2ee/dtds/web-app_2_2.dtd"> 
<web-app>
	<servlet>
		<servlet-name>ajax.Server</servlet-name>
		<servlet-class>ajax.Server</servlet-class>
	</servlet>
	<servlet> 
<servlet-name>ajax.AjaxResponseServlet</servlet-name> 
<servlet-class>ajax.AjaxResponseServlet</servlet-class> 
<load-on-startup>1</load-on-startup> 
</servlet>
	<servlet>
		<servlet-name>ajax.info</servlet-name>
		<servlet-class>ajax.info</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ajax.info</servlet-name>
		<url-pattern>/info</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ajax.Server</servlet-name>
		<url-pattern>/server</url-pattern>
	</servlet-mapping>
	<servlet-mapping> 
<servlet-name>ajax.AjaxResponseServlet</servlet-name> 
<url-pattern>/response</url-pattern> 
</servlet-mapping> 
<welcome-file-list> 
<welcome-file>index.html</welcome-file> 
</welcome-file-list> 
</web-app> 